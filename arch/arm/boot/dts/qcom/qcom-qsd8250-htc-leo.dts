#include "qcom-qsd8250.dtsi"

/ {
	model = "HTC HD2";
	compatible = "htc,leo", "qcom,qsd8250";

	chosen {
        bootargs = "earlycon=efifb console=efifb";

		#address-cells = <1>;
		#size-cells = <1>;
		ranges;
	};

	reserved-memory {
		#address-cells = <1>;
		#size-cells = <1>;
		ranges;

        appsbl: reserved@0 {
			reg = <0x00000000 0x00100000>;
			no-map;
		};

        smem_mem: memory@100000 {
			reg = <0x00100000 0x00100000>;
			no-map;
		};

        uefi_mem: memory@200000 {
			reg = <0x2C000000 0x00100000>;
			no-map;
		};

		display_reserved: framebuffer@0x2a00000 {
			reg = <0x2a00000 0x000C0000>;
			no-map;
		};
	};
};