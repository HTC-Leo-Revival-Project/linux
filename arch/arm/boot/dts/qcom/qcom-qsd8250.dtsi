/dts-v1/;

/ {
	#address-cells = <1>;
	#size-cells = <1>;
	model = "Qualcomm QSD8250";
	compatible = "qcom,qsd8250";
	interrupt-parent = <&intc>;

	chosen { };

	memory {
		device_type = "memory";
		reg = <0x0 0x0>;
	};

	cpus {
		#address-cells = <1>;
		#size-cells = <0>;

		cpu@0 {
			compatible = "qcom,scorpion";
			device_type = "cpu";
			reg = <0>;
			next-level-cache = <&L2>;
		};

		L2: l2-cache {
			compatible = "cache";
			cache-level = <2>;
			cache-unified;
		};
	};

	soc: soc {
		compatible = "simple-bus";
		#address-cells = <1>;
		#size-cells = <1>;
		ranges;

		intc: interrupt-controller@ac000000 {
			compatible = "arm,msm-vic";
			interrupt-controller;
			#interrupt-cells = <3>;
			reg = <0xac000000 0x1000>;
		};

		timer@ac100000 {
			compatible = "qcom,qsd8k-timer";
			interrupts = <7>;
			reg = <0xac100000 0x100>;
			clock-frequency = <19200000>;
		};
	};
};
